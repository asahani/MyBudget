<h1>Listing budget_incomes</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Amount</th>
      <th>Budget</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @budget_incomes.each do |budget_income| %>
      <tr>
        <td><%= budget_income.description %></td>
        <td><%= budget_income.amount %></td>
        <td><%= budget_income.budget_id %></td>
        <td><%= budget_income.account_id %></td>
        <td><%= link_to 'Show', budget_income %></td>
        <td><%= link_to 'Edit', edit_budget_income_path(budget_income) %></td>
        <td><%= link_to 'Destroy', budget_income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Budget income', new_budget_income_path %>
