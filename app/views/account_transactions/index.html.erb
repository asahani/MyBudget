<h1>Listing account_transactions</h1>

<table>
  <thead>
    <tr>
      <th>Account</th>
      <th>Payee</th>
      <th>Budget</th>
      <th>Category</th>
      <th>Amount</th>
      <th>Transaction date</th>
      <th>Comments</th>
      <th>Reconciled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_transactions.each do |account_transaction| %>
      <tr>
        <td><%= account_transaction.account_id %></td>
        <td><%= account_transaction.payee_id %></td>
        <td><%= account_transaction.budget_id %></td>
        <td><%= account_transaction.category_id %></td>
        <td><%= account_transaction.amount %></td>
        <td><%= account_transaction.transaction_date %></td>
        <td><%= account_transaction.comments %></td>
        <td><%= account_transaction.reconciled %></td>
        <td><%= link_to 'Show', account_transaction %></td>
        <td><%= link_to 'Edit', edit_account_transaction_path(account_transaction) %></td>
        <td><%= link_to 'Destroy', account_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account transaction', new_account_transaction_path %>
