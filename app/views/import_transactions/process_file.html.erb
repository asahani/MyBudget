<div class="non-budget-page-content">
	<div id="import-preview-container" style="height: 100%;">
		  <% if flash[:error]%>
		    <div class="alert alert-danger alert-dismissible" role="alert">
				<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
		      <h2><strong>Warning!</strong> An error prohibited this import from being saved:</h2>

		      <ul>
		        <li><%= flash[:error]%></li>
		      </ul>
		    </div>
		<%end%>
	
	
		<div id="import-preview-header">
			<h3>Import Preview</h3>
		</div>	
		<%=render 'preview'%>
		<div class="modal fade" id="newPayeeModal" tabindex="-1" role="dialog" aria-labelledby="newPayeeModalLabel" aria-hidden="true"></div>
		<div class="modal fade" id="transferImportedTxnModal" tabindex="-1" role="dialog" aria-labelledby="transferImportedTxnModalLabel" aria-hidden="true"></div>
	</div>	
</div>