<div class="row" id="import-preview-container">
	<div class="col-md-12">
		<div class="panel panel-default">
			<div class="panel-heading">
					<h3 class="panel-title">Import Preview</h3>
					<ul class="panel-controls">
							<li><a href="#" class="panel-fullscreen"><span class="fa fa-expand"></span></a></li>
					</ul>
			</div>

		  <% if flash[:error]%>
		    <div class="alert alert-danger alert-dismissible" role="alert">
				<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
		      <h2><strong>Warning!</strong> An error prohibited this import from being saved:</h2>
		      <ul>
		        <li><%= flash[:error]%></li>
		      </ul>
		    </div>
			<%end%>

			<%=render 'preview'%>
			<div class="modal fade" id="newPayeeModal" tabindex="-1" role="dialog" aria-labelledby="newPayeeModalLabel" aria-hidden="true"></div>
			<div class="modal fade" id="transferImportedTxnModal" tabindex="-1" role="dialog" aria-labelledby="transferImportedTxnModalLabel" aria-hidden="true"></div>
			<div class="modal fade" id="tagsImportedTxnModal" tabindex="-1" role="dialog" aria-labelledby="tagsImportedTxnModalLabel" aria-hidden="true"></div>
		</div>
	</div>
</div>
